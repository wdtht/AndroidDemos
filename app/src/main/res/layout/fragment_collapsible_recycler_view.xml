<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".network.fragment.CollapsibleRecyclerViewFragment">

        <com.example.androiddemos.view.CollapsibleRecyclerView
            android:id="@+id/collapsibleRecyclerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:expandableItemId="@{@layout/collapsible_recycler_view_header}"
            app:subItemId="@{@layout/recycler_view_item}"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>

    </FrameLayout>
</layout>